{
    "name": "lws/exportactions",
    "description": "Effeciently Export API Data into CSV,PDF Files",
    "homepage": "https://github.com/Phaniraj35/laravel-exporter",
    "license": "MIT",
    "authors": [
        {
            "name": "Phaniraj K",
            "email": "phaniitng@gmail.com",
            "homepage": "https://github.com/Phaniraj35"
        }
    ],
    "require": {
        "ext-zip": "*",
        "illuminate/support": "~5",
        "guzzlehttp/guzzle": "^6.3",
        "soapbox/laravel-formatter": "^3.1",
        "elibyy/tcpdf-laravel": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~8",
        "orchestra/testbench": ">=3.8"
    },
    "autoload": {
        "psr-4": {
            "LWS\\ExportActions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LWS\\ExportActions\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LWS\\ExportActions\\ExportServiceProvider",
                "LWS\\ExportActions\\CustomEventServiceProvider"
            ]
        }
    }
}
