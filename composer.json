{
    "name": "jeroen-g/laravel-packager",
    "description": "A cli tool for creating Laravel packages.",
    "keywords": [
        "Packager",
        "Laravel",
        "package",
        "skeleton"
    ],
    "homepage": "https://github.com/Jeroen-G/laravel-packager",
    "license": "EUPL-1.1",
    "authors": [
        {
            "name": "JeroenG",
            "email": "jeroengjeroeng@gmail.com",
            "homepage": "https://github.com/Jeroen-G"
        }
    ],
    "require": {
        "ext-zip": "*",
        "illuminate/support": "~5",
        "guzzlehttp/guzzle": "^6.3",
        "soapbox/laravel-formatter": "^3.1",
        "elibyy/tcpdf-laravel": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~8",
        "orchestra/testbench": ">=3.8"
    },
    "autoload": {
        "psr-4": {
            "LWS\\ExportActions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LWS\\ExportActions\\Tests\\": "tests"
        }
    },
    "suggest": {
        "sensiolabs/security-checker": "Required to use the security check command"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LWS\\ExportActions\\ExportServiceProvider",
                "LWS\\ExportActions\\CustomEventServiceProvider"
            ]
        }
    }
}
